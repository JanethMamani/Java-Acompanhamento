package Aplicacion;

import java.util.Scanner;

public class Executor {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int x = sc.nextInt();
		int[][] matriz = new int[x][x];
		
		for (int i=0;i<x;i++) {
			for (int j=0;j<x;j++) {
				matriz[i][j]= sc.nextInt();
			}
		}
		
		sc.close();
	}

}
